{
    "name": "flutter_docker",
    "context": "..",
    // "dockerFile": "../Dockerfile",
    "dockerComposeFile": ["../docker-compose.yml"],
    "remoteUser": "root",
    "settings": {
        "terminal.integrated.shell.linux": null
    },
    "runArgs": ["--privileged"],
    "extensions": [
        "dart-code.flutter",
        "dart-code.flutter",
        "felixangelov.bloc",
        "localizely.flutter-intl",

        "bendixma.dart-data-class-generator",
